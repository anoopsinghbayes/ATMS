{
  "name": "atms",
  "version": "0.1.0",
  "description": "A transport management system",
  "scripts": {
    "build": "babel src --out-dir build --watch --source-maps",
    "start": "nodemon ./src/server.js --exec babel-node --debug",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/apollo-starter-kit.git"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "GraphQL",
    "Express",
    "Apollo",
    "Meteor"
  ],
  "author": "anoopsingh bayes,rohit rane",
  "license": "MIT",
  "dependencies": {
    "@risingstack/graffiti-mongoose": "^6.0.0",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.15.2",
    "cors": "^2.8.0",
    "dataloader": "^1.3.0",
    "express": "4.14.0",
    "graphql": "^0.12.3",
    "graphql-compose": "^2.10.1",
    "graphql-compose-mongoose": "^1.10.0",
    "graphql-fields": "^1.0.2",
    "graphql-server-express": "^0.4.2",
    "graphql-subscriptions": "^0.2.0",
    "graphql-tools": "^0.8.0",
    "lodash": "^4.15.0",
    "mongoose": "^4.13.9",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-schema-to-graphql": "^2.4.6",
    "subscriptions-transport-ws": "^0.3.1"
